#include <cstdio>
#include <string>
using namespace std;

int main(){
	int x;
	x=0;
    char age[30];
    printf("Age: ");
    gets(age);     //Function to read string from user.
	
	while( age != "q" )
	    if((int(age)) > x)
	        x=int(age);
	    else
	        printf("error\n");
	else 
        printf("Maxage=%d\n",x);
	
	return 0;
}
